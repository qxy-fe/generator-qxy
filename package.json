{
  "name": "generator-qxy",
  "type": "module",
  "version": "0.3.12",
  "packageManager": "pnpm@7.14.1",
  "keywords": [
    "yeoman-generator",
    "qxy-fe"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "main": "./generators/app/index.js",
  "module": "./generators/app/index.js",
  "types": "./generators/app/index.d.ts",
  "exports": {
    ".": "./generators/app/index.js",
    "./generators/*": "./generators/*",
    "./package.json": "./package.json"
  },
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "tsup",
    "copy": "copyfiles -a -u 1 -V src/*/templates/** generators",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "has-vite": "^0.0.2",
    "has-yarn": "^3.0.0",
    "ora": "^6.1.2",
    "yeoman-generator": "^5.7.0"
  },
  "devDependencies": {
    "@qxy/eslint-config-typescript": "^1.0.10",
    "@qxy/prettier-config": "^1.0.10",
    "@types/yeoman-generator": "^5.2.11",
    "bumpp": "^8.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.26.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{js,ts,json,jsonc,md,yaml,yml}": "eslint --fix"
  },
  "prettier": "@qxy/prettier-config",
  "eslintConfig": {
    "extends": [
      "@qxy/typescript"
    ],
    "root": true
  },
  "eslintIgnore": [
    "generators",
    "types",
    "src/**/templates/**/*"
  ],
  "engines": {
    "node": ">=14.18"
  }
}
