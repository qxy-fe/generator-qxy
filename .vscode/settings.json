{
  "files.associations": {
    "*.yml.ejs": "yaml",
    "*.md.ejs": "markdown",
    "*.json.ejs": "json",
    "*.js.ejs": "plaintext",
    "*.ts.ejs": "plaintext",
    "LICENSE.ejs": "plaintext",
    ".gitignore.ejs": "ignore"
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "eslint.validate": ["javascript", "typescript", "json", "jsonc", "json5", "yaml", "markdown"],
  "editor.codeActionsOnSave": {
    "source.eslint.fixAll": true
  },
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "bumpp",
    "cdnurl",
    "changelogithub",
    "cnpm",
    "execa",
    "lcov",
    "Metas",
    "njsproj",
    "ntnyq",
    "ntvs",
    "pnpm",
    "svgo",
    "tsbuildinfo",
    "tsup",
    "vitepress",
    "vitest",
    "vuepress",
    "yarnrc"
  ]
}
