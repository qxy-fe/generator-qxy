{
  "name": "generator-qxy",
  "type": "module",
  "version": "0.6.5",
  "packageManager": "pnpm@8.2.0",
  "keywords": [
    "yeoman-generator",
    "qxy-fe"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "qxy-fe/generator-qxy",
  "exports": {
    ".": {
      "types": "./generators/app/index.d.ts",
      "import": "./generators/app/index.js"
    },
    "./generators/*": "./generators/*"
  },
  "main": "./generators/app/index.js",
  "module": "./generators/app/index.js",
  "types": "./generators/app/index.d.ts",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "tsup",
    "copy": "copyfiles -a -u 1 -V src/*/templates/** generators",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/yeoman-generator": "^5.2.11",
    "has-vite": "^0.0.4",
    "ora": "^6.3.0",
    "sort-package-json": "^2.4.1",
    "yeoman-generator": "^5.8.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^1.6.1",
    "@ntnyq/prettier-config": "^1.6.1",
    "bumpp": "^9.1.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.38.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "prettier": "^2.8.7",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "pnpm": {
    "overrides": {
      "tsup": "6.6.2"
    }
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,json,jsonc,md,yaml,yml}": "eslint --fix"
  }
}
