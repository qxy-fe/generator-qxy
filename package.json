{
  "name": "generator-qxy",
  "type": "module",
  "version": "0.4.0",
  "packageManager": "pnpm@7.18.0",
  "keywords": [
    "yeoman-generator",
    "qxy-fe"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "main": "./generators/app/index.cjs",
  "module": "./generators/app/index.js",
  "types": "./generators/app/index.d.ts",
  "exports": {
    ".": {
      "require": "./generators/app/index.cjs",
      "import": "./generators/app/index.js",
      "types": "./generators/app/index.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "tsup",
    "copy": "copyfiles -a -u 1 -V src/*/templates/** generators",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release": "bumpp && npm publish"
  },
  "dependencies": {
    "has-vite": "^0.0.2",
    "has-yarn": "^3.0.0",
    "ora": "^6.1.2",
    "sort-package-json": "^2.1.0",
    "yeoman-generator": "^5.7.0"
  },
  "devDependencies": {
    "@qxy/eslint-config-typescript": "^1.1.2",
    "@qxy/prettier-config": "^1.1.2",
    "@types/yeoman-generator": "^5.2.11",
    "bumpp": "^8.2.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.29.0",
    "husky": "^8.0.2",
    "nano-staged": "^0.8.0",
    "prettier": "^2.8.0",
    "tsup": "^6.5.0",
    "typescript": "^4.9.3"
  },
  "nano-staged": {
    "*.{js,ts,json,jsonc,md,yaml,yml}": "eslint --fix"
  },
  "prettier": "@qxy/prettier-config",
  "engines": {
    "node": ">=14.18"
  }
}
