{
  "name": "generator-qxy",
  "type": "module",
  "version": "0.20.0",
  "workspaces": [
    "packages/*"
  ],
  "keywords": [
    "yeoman-generator"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "qxy-fe/generator-qxy",
  "exports": "./generators/app/index.js",
  "main": "./generators/app/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "tsdown",
    "copy": "copyfiles -a -u 1 -V src/*/templates/**/** generators",
    "dev": "tsdown --watch src",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s build lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "nanospinner": "^1.2.2",
    "sort-package-json": "^3.4.0",
    "yeoman-environment": "^4.4.3",
    "yeoman-generator": "^7.5.1"
  },
  "devDependencies": {
    "@generator-qxy/tester": "workspace:*",
    "@ntnyq/eslint-config": "^5.6.0",
    "@ntnyq/prettier-config": "^3.0.1",
    "@types/node": "^24.5.2",
    "@types/yeoman-assert": "^3.1.4",
    "@yeoman/types": "^1.8.0",
    "bumpp": "^10.2.3",
    "copyfiles": "^2.4.1",
    "eslint": "^9.36.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "tinyexec": "^1.0.1",
    "tsdown": "^0.15.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^10.1.1"
  },
  "engines": {
    "node": ">=20.12.0"
  },
  "resolutions": {
    "mem-fs": "^4.1.2"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,yml,yaml,json,jsonc}": "eslint --fix",
    "*.{css,html}": "prettier -uw"
  }
}
