{
  "name": "generator-qxy",
  "type": "module",
  "version": "0.12.0",
  "packageManager": "pnpm@8.11.0",
  "keywords": [
    "yeoman-generator",
    "qxy-fe"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "qxy-fe/generator-qxy",
  "exports": "./generators/app/index.js",
  "main": "./generators/app/index.js",
  "module": "./generators/app/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "tsup",
    "copy": "copyfiles -a -u 1 -V src/*/templates/**/** generators",
    "coverage": "vitest --coverage",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ora": "^7.0.1",
    "sort-package-json": "^2.6.0",
    "yeoman-environment": "^4.1.3",
    "yeoman-generator": "^7.1.1"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "2.0.0-beta.22",
    "@ntnyq/prettier-config": "^1.19.0",
    "@types/yeoman-assert": "^3.1.4",
    "@vitest/coverage-v8": "^1.0.0-beta.5",
    "bumpp": "^9.2.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.54.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^1.0.0-beta.5",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^8.2.0"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "mem-fs": "^4.0.0"
    }
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,json,jsonc,md,yaml,yml}": "eslint --fix"
  }
}
