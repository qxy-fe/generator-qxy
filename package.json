{
  "name": "generator-qxy",
  "type": "module",
  "version": "0.15.3",
  "packageManager": "pnpm@9.12.2",
  "keywords": [
    "yeoman-generator",
    "qxy-fe"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "qxy-fe/generator-qxy",
  "exports": "./generators/app/index.js",
  "main": "./generators/app/index.js",
  "module": "./generators/app/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "tsup",
    "copy": "copyfiles -a -u 1 -V src/*/templates/**/** generators",
    "coverage": "vitest --coverage",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ora": "^8.1.0",
    "sort-package-json": "^2.10.1",
    "yeoman-environment": "^4.4.3",
    "yeoman-generator": "^7.3.3"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.1.1",
    "@ntnyq/generator-tester": "workspace:*",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/yeoman-assert": "^3.1.4",
    "@vitest/coverage-v8": "^2.1.3",
    "@yeoman/types": "^1.5.0",
    "bumpp": "^9.7.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.13.0",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.4",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^10.0.1"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "pnpm": {
    "overrides": {
      "mem-fs": "^4.1.1"
    }
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,cjs,mjs,json,jsonc,md,yaml,yml}": "eslint --fix"
  }
}
