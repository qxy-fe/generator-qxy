{
  "name": "generator-qxy",
  "type": "module",
  "version": "0.3.8",
  "packageManager": "pnpm@7.13.3",
  "keywords": [
    "yeoman-generator",
    "qxy-fe"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "main": "./generators/app/index.js",
  "module": "./generators/app/index.js",
  "types": "./generators/app/index.d.ts",
  "exports": {
    ".": "./generators/app/index.js",
    "./generators/*": "./generators/*",
    "./package.json": "./package.json"
  },
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "run-s clean bundle copy",
    "bundle": "tsc --build",
    "clean": "rimraf generators *.tsbuildinfo",
    "copy": "copyfiles -a -u 1 -V src/*/templates/** generators",
    "dev": "npm run clean && tsc-watch --incremental --onSuccess \"npm run copy\"",
    "lint": "eslint .",
    "lint:script": "",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "dependencies": {
    "ejs": "^3.1.8",
    "has-yarn": "^3.0.0",
    "ora": "^6.1.2",
    "yeoman-generator": "^5.7.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@qxy/eslint-config-typescript": "^1.0.7",
    "@qxy/prettier-config": "^1.0.7",
    "@types/ejs": "^3.1.1",
    "@types/yeoman-generator": "^5.2.11",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "copyfiles": "^2.4.1",
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{js,ts,json,jsonc,md,yaml,yml}": "eslint --fix"
  },
  "prettier": "@qxy/prettier-config",
  "eslintConfig": {
    "extends": [
      "@qxy/typescript"
    ],
    "root": true
  },
  "eslintIgnore": [
    "generators",
    "types",
    "src/**/templates/**/*"
  ],
  "engines": {
    "node": ">=14.18"
  }
}
