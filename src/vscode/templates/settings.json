{
  "files.eol": "\n",
  "files.encoding": "utf8",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.formatOnSave": true,
  "eslint.validate": [
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact",
    "vue",
    "yaml",
    "html",
    "markdown",
    "json",
    "jsonc",
    "json5"
  ],
  "editor.codeActionsOnSave": {
    "source.eslint.fixAll": true
  },
  "[javascript]": {
    "editor.formatOnSave": false
  }
}
