{
  "name": "generator-qxy",
  "version": "0.3.6",
  "keywords": [
    "yeoman-generator",
    "qxy-fe"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "type": "module",
  "exports": "./generators/app/index.js",
  "types": "generators/app/index.d.ts",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "run-s clean bundle copy",
    "bundle": "tsc --build",
    "clean": "rimraf generators *.tsbuildinfo",
    "copy": "copyfiles -a -u 1 -V src/*/templates/** generators",
    "dev": "npm run clean && tsc-watch --incremental --onSuccess \"npm run copy\"",
    "lint": "run-p lint:*",
    "lint:script": "eslint --ext .ts src",
    "lint:cspell": "cspell \"**\"",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{json,md}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "prettier": "@qxy/prettier-config",
  "eslintConfig": {
    "extends": [
      "@qxy/typescript"
    ],
    "root": true
  },
  "eslintIgnore": [
    "generators",
    "types",
    "src/**/templates/**/*"
  ],
  "dependencies": {
    "ejs": "^3.1.7",
    "has-yarn": "^3.0.0",
    "ora": "^6.1.0",
    "sort-package-json": "^1.55.0",
    "yeoman-generator": "^5.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@qxy/eslint-config-typescript": "^0.1.1",
    "@qxy/prettier-config": "^0.3.1",
    "@types/ejs": "^3.1.0",
    "@types/yeoman-generator": "^5.2.10",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "copyfiles": "^2.4.1",
    "cspell": "^5.19.7",
    "eslint": "^8.14.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.6.3"
  },
  "packageManager": "pnpm@6.32.6",
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  }
}
