{
  "name": "generator-qxy",
  "type": "module",
  "version": "0.7.1",
  "packageManager": "pnpm@8.6.6",
  "keywords": [
    "yeoman-generator",
    "qxy-fe"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "qxy-fe/generator-qxy",
  "exports": {
    ".": {
      "types": "./generators/app/index.d.ts",
      "import": "./generators/app/index.js"
    },
    "./*": "./*"
  },
  "main": "./generators/app/index.js",
  "module": "./generators/app/index.js",
  "types": "./generators/app/index.d.ts",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "run-s build:lib build:dts",
    "build:dts": "tsc --emitDeclarationOnly --declaration",
    "build:lib": "tsup",
    "build:test": "NODE_ENV=test pnpm run build",
    "copy": "copyfiles -a -u 1 -V src/*/templates/** generators",
    "coverage": "vitest --coverage",
    "dev": "tsup --watch src",
    "lint": "ESLINT_USE_FLAT_CONFIG=true eslint --max-warnings 0 .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@types/yeoman-generator": "^5.2.11",
    "ora": "^6.3.1",
    "sort-package-json": "^2.5.0",
    "yeoman-generator": "^5.9.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "2.0.0-beta.11",
    "@ntnyq/prettier-config": "^1.9.0",
    "@types/yeoman-assert": "^3.1.1",
    "@types/yeoman-test": "^4.0.3",
    "@vitest/coverage-v8": "^0.32.4",
    "bumpp": "^9.1.1",
    "copyfiles": "^2.4.1",
    "eslint": "^8.44.0",
    "eslint-define-config": "^1.21.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "vitest": "^0.32.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^7.4.0"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,json,jsonc,md,yaml,yml}": "eslint --fix"
  }
}
