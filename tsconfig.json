{
  "compilerOptions": {
    "baseUrl": ".",
    "outDir": "generators",
    "module": "ESNext",
    "target": "ES2022",
    "moduleResolution": "Bundler",
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "removeComments": true,
    "strictNullChecks": true
  },
  "include": ["./src", "./tests", "./vitest.config.ts", "./tsup.config.ts"]
}
