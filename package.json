{
  "name": "generator-qxy",
  "version": "0.3.0",
  "keywords": [
    "yeoman-generator",
    "qxy-fe"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "type": "module",
  "exports": "./generators/app/index.js",
  "types": "generators/app/index.d.ts",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "npm run clean && tsc && npm run copy",
    "clean": "rimraf generators *.tsbuildinfo",
    "copy": "copyfiles -a -u 1 -V src/*/templates/** generators",
    "dev": "npm run clean && tsc-watch --incremental --onSuccess \"npm run copy\"",
    "lint": "eslint --ext .ts src",
    "lint:cspell": "cspell **",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix",
    "*.{json,md}": "prettier --write",
    "package.json": "sort-package-json"
  },
  "prettier": "@qxy/prettier-config",
  "eslintConfig": {
    "extends": [
      "@qxy/prettier",
      "plugin:@typescript-eslint/recommended"
    ],
    "rules": {
      "no-underscore-dangle": "off"
    },
    "root": true
  },
  "eslintIgnore": [
    "generators",
    "types",
    "src/**/templates/**/*"
  ],
  "dependencies": {
    "ejs": "^3.1.6",
    "has-yarn": "^3.0.0",
    "ora": "^6.1.0",
    "sort-package-json": "^1.54.0",
    "yeoman-generator": "^5.6.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@qxy/eslint-config-prettier": "^0.2.15",
    "@qxy/prettier-config": "^0.3.0",
    "@types/ejs": "^3.1.0",
    "@types/yeoman-generator": "^5.2.9",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "conventional-changelog": "^3.1.25",
    "conventional-changelog-cli": "^2.2.2",
    "copyfiles": "^2.4.1",
    "cspell": "^5.18.5",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "tsc-watch": "^4.6.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  }
}
