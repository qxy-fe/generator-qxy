{
  "files.associations": {
    "*.yml.ejs": "yaml",
    "*.md.ejs": "markdown",
    "*.json.ejs": "json",
    "*.js.ejs": "plaintext",
    "*.ts.ejs": "plaintext",
    "LICENSE.ejs": "plaintext",
    ".gitignore.ejs": "ignore"
  },
  "editor.formatOnSave": true,
  "eslint.experimental.useFlatConfig": true,
  "eslint.validate": [
    "vue",
    "yaml",
    "html",
    "json",
    "jsonc",
    "json5",
    "markdown",
    "javascript",
    "typescript",
    "javascriptreact",
    "typescriptreact"
  ],
  "editor.codeActionsOnSave": {
    "source.eslint.fixAll": true
  },
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "bumpp",
    "cdnurl",
    "changelogithub",
    "cnpm",
    "execa",
    "lcov",
    "Metas",
    "njsproj",
    "npmrc",
    "ntnyq",
    "ntvs",
    "pnpm",
    "svgo",
    "tsbuildinfo",
    "tsup",
    "vitepress",
    "vitest",
    "vuepress",
    "yarnrc"
  ]
}
