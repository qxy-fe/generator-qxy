{
  "name": "generator-qxy",
  "type": "module",
  "version": "0.16.0",
  "packageManager": "pnpm@9.15.0",
  "keywords": [
    "yeoman-generator"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "repository": "qxy-fe/generator-qxy",
  "exports": "./generators/app/index.js",
  "main": "./generators/app/index.js",
  "module": "./generators/app/index.js",
  "files": [
    "generators"
  ],
  "scripts": {
    "build": "tsup",
    "copy": "copyfiles -a -u 1 -V src/*/templates/**/** generators",
    "coverage": "vitest --coverage",
    "dev": "tsup --watch src",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "ora": "^8.1.1",
    "sort-package-json": "^2.12.0",
    "yeoman-environment": "^4.4.3",
    "yeoman-generator": "^7.4.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.9.2",
    "@ntnyq/generator-tester": "workspace:*",
    "@ntnyq/prettier-config": "^1.22.0",
    "@types/yeoman-assert": "^3.1.4",
    "@vitest/coverage-v8": "^3.0.0-beta.2",
    "@yeoman/types": "^1.5.0",
    "bumpp": "^9.9.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "prettier": "^3.4.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^3.0.0-beta.2",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^10.0.1"
  },
  "engines": {
    "node": ">=18.18.0"
  },
  "pnpm": {
    "overrides": {
      "mem-fs": "^4.1.2"
    }
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,json,jsonc,md,yaml,yml}": "eslint --fix"
  }
}
